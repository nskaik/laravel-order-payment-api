                    "response": [
                        {
                            "header": [],
                            "code": 200,
                            "body": "{\n  \"id\": 1,\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"email_verified_at\": null,\n  \"created_at\": \"2024-02-03T12:00:00.000000Z\",\n  \"updated_at\": \"2024-02-03T12:00:00.000000Z\"\n}",
