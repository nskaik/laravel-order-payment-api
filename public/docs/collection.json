                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application\/json"
                            },
                            {
                                "key": "Accept",
                                "value": "application\/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\"email\":\"john@example.com\",\"password\":\"password123\"}"
                        },
                        "description": "Authenticate a user with email and password to receive a JWT token.",
                        "auth": {
                            "type": "noauth"
                        }
                    },
                    "response": [
                        {
                            "header": [],
                            "code": 200,
                            "body": "{\n  \"token\": \"eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJpc3MiOiJodHRwOi8vbG9jYWxob3N0OjgwMDAvYXBpL2F1dGgvbG9naW4iLCJpYXQiOjE3MDcwMDAwMDAsImV4cCI6MTcwNzAwMzYwMCwibmJmIjoxNzA3MDAwMDAwLCJqdGkiOiJhYmNkZWYxMjM0NTYiLCJzdWIiOiIxIiwicHJ2IjoiMjNiZDVjODk0OWY2MDBhZGIzOWU3MDFjNDAwODcyZGI3YTU5NzZmNyJ9.example_signature\",\n  \"user\": {\n    \"id\": 1,\n    \"name\": \"John Doe\",\n    \"email\": \"john@example.com\",\n    \"email_verified_at\": null,\n    \"created_at\": \"2024-02-03T12:00:00.000000Z\",\n    \"updated_at\": \"2024-02-03T12:00:00.000000Z\"\n  }\n}",
                            "name": "Successful login"
                        },
                        {
                            "header": [],
                            "code": 401,
                            "body": "{\n  \"error\": \"Invalid credentials\"\n}",
                            "name": "Invalid credentials"
                        },
                        {
                            "header": [],
                            "code": 422,
                            "body": "{\n  \"errors\": {\n    \"email\": [\"The email field is required.\"],\n    \"password\": [\"The password field is required.\"]\n  }\n}",
                            "name": "Validation error"
                        }
                    ]
                }
            ]
        },
        {
